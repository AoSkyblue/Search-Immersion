{
  "manifest_version": 3,
  "name": "Search Immersion",
  "version": "0.9.2.2",
  "description": "リリーステスト版",
  "permissions": [
    "storage",
    "tabs",
    "scripting",
    "history",
    "identity"
  ],
  "oauth2": {
    "client_id": "533899234098-j3ffugf836jg7efb93e03mb329kpnbdv.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.readonly"
    ]
  },
  "host_permissions": [
    "*://*/*",
    "https://raw.githubusercontent.com/*"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.google.com/*",
        "*://www.google.co.jp/*"
      ],
      "css": [
        "styles.css"
      ],
      "js": [
        "i18n/ja.js",
        "i18n/en.js",
        "i18n/ko.js",
        "content.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "components/*.js"
      ],
      "matches": [
        "*://www.google.com/*",
        "*://www.google.co.jp/*"
      ]
    }
  ]
}